<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="RecyclerMessageView">
        <attr name="message" format="string"/>
    </declare-styleable>
    <declare-styleable name="CarrotMarketAppbar">
        <attr name="back_button" format="boolean"/>
        <attr name="android:entries"/>
        <attr name="title"/>
    </declare-styleable>
    <declare-styleable name="MyTownButtonView">
        <attr name="is_active" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="IconCounterView">
        <attr name="type"  format="enum">
            <enum name="favorite" value="0"/>
            <enum name="chat" value="1"/>
            <enum name="multi_chat" value="2"/>
        </attr>
        <attr name="count" format="integer"/>
    </declare-styleable>
</resources>